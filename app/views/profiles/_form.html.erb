<%= form_with(model: profile, local: true) do |f| %>
  <% if profile.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(profile.errors.count, "error") %> prohibited this profile from being saved:</h2>

      <ul>
        <% profile.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

<div class="field">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>

  <div class="input-group">
    <div class="custom-file">
      <%= f.file_field :profile_image, class: 'custom-file-input', id: 'inputGroupFile01'  %>
      <%= f.label :profile_image, class: 'custom-file-label', for: 'inputGroupFile01' %><br />
    </div>
  </div>

  <div class="field">
    <%= f.label :contact %>
    <%= f.text_field :contact %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
